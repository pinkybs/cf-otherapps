						{%/if%}
					{%/if%}
				{%/if%}

				{% if ($rowdata.sid==10)%}
					{%if ($rowdata.count>0)%}
					   {%if $hasBreakCar == 1 %}
						<a href='{% "$baseUrl/mobile/parking/userepair"|mixiurl %}'>使用する</a>&nbsp;
						{%else%}
						<span style="color:#999;">使用する</span>&nbsp;
						{%/if%}
					{%elseif ($rowdata.count==0) %}
						{%if $rowdata.price > $userAsset%}
							ｱｲﾃﾑｼｮｯﾌﾟで購入&nbsp;
						{%else%}
							<a href='{% "$baseUrl/mobile/parking/buyitem?parking_cid=10"|mixiurl %}'>ｱｲﾃﾑｼｮｯﾌﾟで購入</a>&nbsp;
						{%/if%}
					{%/if%}
				{%/if%}

				{% if ($rowdata.sid==11)%}
					{%if $rowdata.count > 0%}
						{% if ($haveFriends != null && $haveFriends != '')%}
							<a href='{% "$baseUrl/mobile/parking/useyankee"|mixiurl %}'>使用する</a>&nbsp;
						{%else%}
							使用する&nbsp;
						{%/if%}
					{%/if%}
					{%if $rowdata.count == 0%}
						{%if $rowdata.price > $userAsset%}
							ｱｲﾃﾑｼｮｯﾌﾟで購入&nbsp;
						{%else%}
							<a href='{% "$baseUrl/mobile/parking/buyitem?parking_cid=11"|mixiurl %}'>ｱｲﾃﾑｼｮｯﾌﾟで購入</a>&nbsp;
						{%/if%}
					{%/if%}
