					{%/if%}
					{%if $rowdata.count==0%}
						{%if ($last_check_time<=1)%}
							使用中&nbsp;
						{%elseif $rowdata.price<=$userAsset%}
							<a href='{% "$baseUrl/mobile/parking/buyitem?parking_cid=6"|mixiurl %}'>ｱｲﾃﾑｼｮｯﾌﾟで購入</a>&nbsp;
						{%else%}
							ｱｲﾃﾑｼｮｯﾌﾟで購入&nbsp;
						{%/if%}
					{%/if%}
				{%/if%}

				{% if ($rowdata.sid==7)%}
					{%if ($rowdata.count>0)%}
						{%if ($rowdata.insurance_card == 0)%}
							<a href='{% "$baseUrl/mobile/parking/useitemconform?parking_id=7"|mixiurl %}'>使用する</a>&nbsp;
						{%/if%}
						{%if ($rowdata.insurance_card == 1)%}
							使用中&nbsp;
						{%/if%}
					{%/if%}
					{%if ($rowdata.count == 0)%}
						{%if ($rowdata.insurance_card == 1)%}
							使用中&nbsp;
						{%elseif $rowdata.price<=$userAsset%}
							<a href='{% "$baseUrl/mobile/parking/buyitem?parking_cid=7"|mixiurl %}'>ｱｲﾃﾑｼｮｯﾌﾟで購入</a>&nbsp;
						{%else%}
							ｱｲﾃﾑｼｮｯﾌﾟで購入&nbsp;
