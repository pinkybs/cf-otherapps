							<a href='{% "$baseUrl/mobile/parking/buyitem?parking_cid=4"|mixiurl %}'>ｱｲﾃﾑｼｮｯﾌﾟで購入</a>&nbsp;
						{%/if%}
					{%/if%}
				{%/if%}

				{% if ($rowdata.sid==5) %}
					{%if ($rowdata.count>0)%}
						<a href='{% "$baseUrl/mobile/parking/useitemconform?parking_id=5"|mixiurl %}'>使用する</a>&nbsp;
