{%include file="parking/common/header.phtml"%}

<div id="itemList">

	<img src="{%$staticUrl%}/apps/parking/mimg/spacer.gif" width="1" height="5" /><br />

	<div class="headline" style="background:#cd424a;">
		<span style="color:#fff;">ｱｲﾃﾑ一覧</span><br />
	</div><!--/.headline-->

	<div class="description">
		<img src="{%$staticUrl%}/apps/parking/mimg/spacer.gif" width="1" height="2" /><br />
		ｱｲﾃﾑを使って､ｹﾞｰﾑを有利に進めましょう{% emoji code="F99F"%}<br />
	</div><!--/.description-->

	<div style="background:#fcc;text-align:center;"><img src="{%$staticUrl%}/apps/parking/mimg/dot.gif" width="228" height="1" /></div>
	{%foreach from=$userItemInfo item=rowdata key=index%}
		<div class="section" {%if ($index%2)==0 %}style="background:#fee;"{%else%}style="background:#ffe;"{%/if%}>
			<span style="color:#cd424a;">{% emoji code="F9A3"%}{%$rowdata.name%}×{%$rowdata.count%}</span><br />
			<table width="220" cellpadding="2" cellspacing="1" border="0" align="center" style="text-align:center;">
				<tr>
					<td style="text-align:left;"><div style="font-size:x-small;">{%$rowdata.introduce%}</div></td>
				</tr>
			</table>
			<div style="color:#999;text-align:right;">
				{% if ($rowdata.sid==1) %}
					{% if ($havaFreePark==0)%}
						使用中&nbsp;
					{% else %}
						{%if ($rowdata.count>0)%}
							<a href='{% "$baseUrl/mobile/parking/useitemconform?parking_id=1"|mixiurl %}'>使用する</a>&nbsp;
						{%elseif ($rowdata.count==0) %}
							{%if $rowdata.price>$userAsset%}
								ｱｲﾃﾑｼｮｯﾌﾟで購入&nbsp;
							{%else%}
								<a href='{% "$baseUrl/mobile/parking/buyitem?parking_cid=1"|mixiurl %}'>ｱｲﾃﾑｼｮｯﾌﾟで購入</a>&nbsp;
							{%/if%}
						{%/if%}
					{%/if%}
				{% /if %}

				{%if $rowdata.sid==2 %}
					{%if ($rowdata.count>0)%}
						使用可&nbsp;
					{%elseif ($rowdata.count==0) %}
						{%if $rowdata.price > $userAsset%}
							ｱｲﾃﾑｼｮｯﾌﾟで購入&nbsp;
						{%else%}
							<a href='{% "$baseUrl/mobile/parking/buyitem?parking_cid=2"|mixiurl %}'>ｱｲﾃﾑｼｮｯﾌﾟで購入</a>&nbsp;
						{%/if%}
					{%/if%}
				{%/if%}

				{%if $rowdata.sid==3%}
					{%if $rowdata.count>0%}
						{%if ($last_bribery_time<=3)%}使用中&nbsp;{%/if%}
