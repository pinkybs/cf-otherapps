					{%if $rowdata.count==0%}
						{%if ($last_bribery_time<=3)%}
							使用中&nbsp;
						{%elseif $rowdata.price<=$userAsset%}
							<a href='{% "$baseUrl/mobile/parking/buyitem?parking_cid=3"|mixiurl %}'>ｱｲﾃﾑｼｮｯﾌﾟで購入</a>&nbsp;
						{%else%}
							ｱｲﾃﾑｼｮｯﾌﾟで購入&nbsp;
						{%/if%}
					{%/if%}
				{%/if%}

				{%if $rowdata.sid==4 %}
					{%if ($rowdata.count>0)%}
						使用可&nbsp;
